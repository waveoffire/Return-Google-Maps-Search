{
    "manifest_version": 3,
    "name": "Return Google Maps Search",
    "version": "1.0",
    "description": "restores the google maps button to google search",
    "permissions": [],
    "icons": {
        "16": "images/googlemaps.png",
        "32": "images/googlemaps.png",
        "48": "images/googlemaps.png",
        "128": "images/googlemaps.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "*://*/*"
            ],
            "include_globs": [
                "*://*.google.*/*"
            ]
        }
    ]
}